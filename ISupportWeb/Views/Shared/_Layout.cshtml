            width: 100%;
            object-fit: contain;
            margin: auto;
        }

        @@media only screen and (max-width: 480px) {
            .Feature-tiem > a img, .product-image > a img {
                height: auto;
                min-height: 165px;
                max-height: 165px;
            }

            .shop-item > a img {
                height: auto !important;
                min-height: 165px;
                max-height: 165px;
            }
        }

        .hovereffect {
            width: 100%;
            overflow: hidden;
            position: relative;
            text-align: center;
            cursor: default;
            border-radius: 5px;
        }

            .hovereffect:hover img {
                -ms-transform: scale(2.2);
                -webkit-transform: scale(2.2);
                transform: scale(2.2);
            }

            .hovereffect img {
                display: block;
                position: relative;
                -webkit-transition: all .4s linear;
                transition: all 1.2s linear;
            }
    </style>


